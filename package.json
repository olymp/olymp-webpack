{
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "version": "15.2.8",
  "name": "olymp-webpack",
  "description": "",
  "scripts": {
    "up": "npm-check -u",
    "init": "lerna init --independent",
    "lp": "lerna publish --registry=https://registry.npmjs.org/",
    "lpf": "lerna publish --registry=https://registry.npmjs.org/ --force-publish",
    "lb": "lerna bootstrap",
    "li": "lerna exec --concurrency 1 -- npm i",
    "lu": "lerna exec --concurrency 1 -- npm-check -u",
    "lc": "del 'packages/*/package-lock.json' && del 'packages/*/node_modules'"
  },
  "engines": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/olymp/olymp-webpack.git"
  },
  "keywords": [
    "react",
    "universal",
    "javascript",
    "express",
    "webpack"
  ],
  "author": "Benjamin Kniffler <bkniffler@me.com>",
  "contributors": [
    "Benjamin Kniffler <bkniffler@me.com>",
    "Philipp Achenbach <olymp@philipp-achenbach.de>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/olymp/olymp-webpack/issues"
  },
  "homepage": "https://github.com/olymp/olymp-webpack#readme",
  "dependencies": {
    "webpack-bundle-analyzer": "^2.10.0"
  },
  "devDependencies": {
    "lerna": "^2.9.0"
  }
}
