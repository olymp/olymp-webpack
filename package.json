{
  "private": true,
  "workspaces": ["packages/*"],
  "version": "15.2.8",
  "name": "olymp-webpack",
  "description": "",
  "scripts": {
    "up": "yarn upgrade-interactive --latest",
    "init": "lerna init --independent",
    "watch": "olymp-lerna watch",
    "lp": "lerna publish --registry=https://registry.npmjs.org/",
